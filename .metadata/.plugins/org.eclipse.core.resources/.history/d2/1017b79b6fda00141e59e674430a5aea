package maxcity;

public class driver {

	public static void main(String[] args) {
		graph g = new graph();
		g.addEdge("A", "B");
		g.addEdge("B", "C");
		g.addEdge("B", "D");
		g.addEdge("B", "A");
		g.addEdge("B", "E");
		g.addEdge("B", "F");
		g.addEdge("C", "A");
		g.addEdge("D", "C");
		g.addEdge("E", "B");
		g.addEdge("F", "B");
	dfsIterator maxc = new dfsIterator(g,"A");
	StringBuilder sb = new StringBuilder();
	while (maxc.hasNext()){
		sb.append(' ').append(maxc.next());
	}
	System.out.println(sb.substring(1));
	
	}
}